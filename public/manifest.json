{
  "manifest_version": 3,
  "name": "Wealthsimple Transfer Logs Chrome Extension",
  "version": "1.0.0",
  "description": "A simple Chrome extension to view account transfer logs",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["https://my.wealthsimple.com/app/home"],
      "js": ["contentScript.js"],
      "run_at": "document_end"
    }
  ]
}